#include "main.h"
/**
  * _strncpy-copies string
  * @dest:destination
  : @src: source
  : @n: elements to be copied
  : Return:counter
  */
char *_strncpy(char *dest, char *src, int n)
{
	int counter;

	counter = 0;
	while (src[counter] != '\0' && counter < n)
	{
		dest[counter] = src[counter];
			counter++;
	}
	while (counter < n)
	{
		dest[counter] = '\0';
		counter++;
	}
	return (counter);
}
